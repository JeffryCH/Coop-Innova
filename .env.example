# Configuración de Base de Datos para Coop-Innova
# Copia este archivo como .env si quieres personalizar las credenciales

# Configuración principal de MySQL
DB_HOST=localhost
DB_NAME=coop_innova

# Credenciales primarias (se prueba primero)
DB_USER_1=root
DB_PASS_1=

# Credenciales de respaldo (se prueba si la primera falla)
DB_USER_2=root
DB_PASS_2=

# Puerto del servidor web (puerto Apache)
APACHE_PORT=99

# Puerto de MySQL
DB_PORT=3306

# Puerto para Node.js (backend)
PORT=5000

# Configuración adicional
NODE_ENV=development